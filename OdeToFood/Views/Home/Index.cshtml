@model OdeToFood.ViewModels.HomeViewModel
@{
    ViewBag.Title = "Home";
}
<h1>Welcome!</h1>

<div>
    <table>
        @foreach (var restaurant in Model.Restaurants)
            {
            <tr>
                <td>@Html.ActionLink(restaurant.Id.ToString(), "Details", new { id = restaurant.Id })</td>
                <td>@restaurant.Name </td>
            </tr>
        }

    </table>
</div>
@section footer {
    <h3>@Model.CurrentGreeting</h3>
}